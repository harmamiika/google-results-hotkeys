{
  "manifest_version": 3,
  "name": "Google search results hotkeys",
  "version": "1.0",
  "icons": {},
  "description": "Use number keys to open search results",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["index.js"]
    }
  ],
  "permissions": ["activeTab"]
}
